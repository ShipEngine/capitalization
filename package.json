{
  "name": "@shipengine/capitalization",
  "version": "1.2.0",
  "description": "String capitalization functions with special cases for certain ShipEngine words and phrases",
  "keywords": [
    "shipengine",
    "string",
    "casing",
    "capitalization",
    "snake-case",
    "kebab-case",
    "camel-case",
    "pascal-case",
    "title-case",
    "sentence-case"
  ],
  "author": {
    "name": "ShipEngine",
    "email": "support@shipengine.com",
    "url": "https://shipengine.com"
  },
  "homepage": "https://shipengine.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/ShipEngine/capitalization.git"
  },
  "license": "Apache-2.0",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib"
  ],
  "engines": {
    "node": ">=8"
  },
  "engineStrict": true,
  "scripts": {
    "clean": "shx rm -rf .nyc_output coverage lib",
    "lint": "npm run lint:typescript && npm run lint:javascript",
    "coverage": "nyc node_modules/mocha/bin/mocha",
    "lint:typescript": "tslint -p tsconfig.json",
    "lint:javascript": "eslint test",
    "build": "tsc",
    "watch": "tsc --watch",
    "test": "mocha && npm run lint",
    "upgrade": "npm-check -u",
    "bump": "bump --tag --push --all",
    "release": "npm run upgrade && npm run clean && npm run build && npm test && npm run bump && npm publish"
  },
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.6.8",
    "chai": "^4.2.0",
    "eslint": "^6.1.0",
    "eslint-config-modular": "^7.0.0",
    "mocha": "^6.2.0",
    "npm-check": "^5.9.0",
    "nyc": "^15.0.1",
    "shx": "^0.3.2",
    "source-map-support": "^0.5.12",
    "tslint": "^5.17.0",
    "tslint-modular": "^1.4.1",
    "typescript": "^3.5.3",
    "typescript-tslint-plugin": "^0.5.4",
    "version-bump-prompt": "^5.0.4"
  },
  "dependencies": {}
}
